generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model ApiKey {
  id            Int      @id @default(autoincrement())
  name          String   @unique
  encrypted_key String
  createdAt     DateTime @default(now()) @map("created_at")

  @@map("api_keys")
}
